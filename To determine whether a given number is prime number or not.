#include <stdio.h>
int main() {
    int a, i, count=0;
    printf("Enter a number: ");
    scanf("%d", &a);
    
    for(i=1;i<=a;i++) {
        if (a%i==0) {
            count++;
        }
    }
    
    if (count==2) {
        printf("This is a prime number.");
    }
    else {
        printf("This is not a prime number.");
    }
    return 0;
}
